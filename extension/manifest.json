{
  "manifest_version": 3,
  "name": "CocBot",
  "version": "1.0.0",
  "description": "A Chrome extension that helps you read and understand webpages with the power of AI.",
  "icons": {
    "16": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "permissions": ["storage", "activeTab", "sidePanel", "scripting", "identity"],
  "host_permissions": ["https://api.openai.com/*"],
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "icons/logo.png",
      "48": "icons/logo.png",
      "128": "icons/logo.png"
    }
  },
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"],
      "css": ["styles/content.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/*", "sidebar.html", "styles/*", "components/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "oauth2": {
    "client_id": "833310676000-755pdr3r9isck33epnmom0e8v2vfcsvs.apps.googleusercontent.com",
    "scopes": ["openid", "email", "profile"]
  },
  "key": ""
}
